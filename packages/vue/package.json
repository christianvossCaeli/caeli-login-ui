{
  "name": "@christianvosscaeli/login-vue",
  "version": "1.0.0",
  "description": "Vue 3 components for Caeli login pages",
  "author": "Caeli Wind",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
