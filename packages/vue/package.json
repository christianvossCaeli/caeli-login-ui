      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.0",
    "@azure/msal-browser": "^4.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvossCaeli/caeli-login-ui.git",
    "directory": "packages/vue"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
